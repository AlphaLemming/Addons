<GuiXml>
	<Font name="AGFont" font="$(MEDIUM_FONT)|18"/>
	<Font name="AGFontSmall" font="$(BOLD_FONT)|14"/>
	<Font name="AGFontBold" font="$(BOLD_FONT)|22"/>
    <Controls>
		<Backdrop name="AG_UI_ButtonBg" alpha="0" centerColor="40FF0000" edgeColor="60FF0000" clampedToScreen="true" movable="true" mouseEnabled="true">
			<Anchor point="3" relativePoint="3" relativeTo="GuiRoot" offsetX="50" offsetY="100"/>
			<Dimensions x="60" y="60"/><Edge edgeSize="1"/><OnInitialized>self:SetParent(ZO_MainMenu)</OnInitialized>
			<OnMouseEnter>WINDOW_MANAGER:SetMouseCursor(12);self:SetAlpha(1)</OnMouseEnter>
			<OnMouseExit>WINDOW_MANAGER:SetMouseCursor(0);self:SetAlpha(0)</OnMouseExit>
			<OnMouseUp>AG4.account.button={self:GetLeft(),self:GetTop()}</OnMouseUp>
		</Backdrop>
		<Button name="AG_UI_Button" topmost="true"><Anchor point="128" relativePoint="128" relativeTo="AG_UI_ButtonBg"/>
			<Dimensions x="40" y="40"/><OnInitialized>self:SetParent(ZO_MainMenu)</OnInitialized><Textures normal="AlphaGearX2/ag.dds"/>
			<OnMouseEnter>AG4.Tooltip(self,true)</OnMouseEnter>
			<OnMouseExit>AG4.Tooltip(self,false)</OnMouseExit>
			<OnClicked>AG4.ShowMain()</OnClicked>
		</Button>

		<TopLevelControl name="AG_Panel" hidden="true" clampedToScreen="true" movable="true" mouseEnabled="true" allowBringToTop="true">
			<Anchor point="128" relativePoint="128" relativeTo="GuiRoot"/>
			<OnMouseUp>AG4.account.pos={self:GetLeft(),self:GetTop()}</OnMouseUp><Dimensions x="670" y="819"/>
			<Controls>
				<Backdrop name="$(parent)Bg" centerColor="282828" edgeColor="00000000"><AnchorFill/><Edge edgeSize="1"/></Backdrop>
				<Backdrop name="$(parent)Menu" centerColor="598527" edgeColor="00000000" hidden="true" tier="2">
					<Anchor point="3" relativePoint="3" relativeTo="$(parent)"/><Dimensions x="250"/><Edge edgeSize="1"/>
					<Controls>
						<Button name="$(parent)Button1" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="ZoFontGame" hidden="true">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="10" offsetY="10"/>
							<FontColors normalColor="FFFFFF" mouseOverColor="FFAA33"/><Dimensions x="230"/>
							<OnInitialized>self:SetText(AG4[GetCVar('language.2')].Copy)</OnInitialized>
							<OnClicked>AG4.MenuAction(1)</OnClicked>
						</Button>
						<Button name="$(parent)Button2" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="ZoFontGame" hidden="true">
							<Anchor point="3" relativePoint="6" relativeTo="$(parent)Button1"/>
							<FontColors normalColor="FFFFFF" mouseOverColor="FFAA33"/><Dimensions x="230"/>
							<OnInitialized>self:SetText(AG4[GetCVar('language.2')].Paste)</OnInitialized>
							<OnClicked>AG4.MenuAction(2)</OnClicked>
						</Button>
						<Button name="$(parent)Button3" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="ZoFontGame" hidden="true">
							<Anchor point="3" relativePoint="6" relativeTo="$(parent)Button2"/>
							<FontColors normalColor="FFFFFF" mouseOverColor="FFAA33"/><Dimensions x="230"/>
							<OnInitialized>self:SetText(AG4[GetCVar('language.2')].Insert)</OnInitialized>
							<OnClicked>AG4.MenuAction(3)</OnClicked>
						</Button>
						<Button name="$(parent)Button4" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="ZoFontGame" hidden="true">
							<Anchor point="3" relativePoint="6" relativeTo="$(parent)Button3"/>
							<FontColors normalColor="FFFFFF" mouseOverColor="FFAA33"/><Dimensions x="230"/>
							<OnInitialized>self:SetText(AG4[GetCVar('language.2')].Clear)</OnInitialized>
							<OnClicked>AG4.MenuAction(4)</OnClicked>
						</Button>
					</Controls>
				</Backdrop>
				<Label name="$(parent)Headline" font="AGFont" color="FFFFFF">
					<Anchor point="1" relativePoint="1" relativeTo="$(parent)" offsetY="6"/>
					<OnInitialized>self:SetText("|cFFAA33"..AG4.name.."|r "..AG4.version)</OnInitialized>
				</Label>
				<Button name="$(parent)CloseButton" clickSound="Click">
					<Anchor point="9" relativePoint="9" relativeTo="$(parent)" offsetX="-10" offsetY="4"/>
					<Dimensions y="25" x="25"/>
					<Textures normal="esoui/art/buttons/decline_up.dds" pressed="esoui/art/buttons/decline_down.dds" mouseOver="esoui/art/buttons/decline_over.dds"/>
					<OnClicked>AG4.ShowMain()</OnClicked>
				</Button>
				<Button name="$(parent)OptionButton" clickSound="Click">
					<Anchor point="8" relativePoint="2" relativeTo="$(parent)CloseButton"/>
					<Dimensions y="35" x="35"/>
					<Textures normal="esoui/art/chatwindow/chat_options_up.dds" pressed="esoui/art/chatwindow/chat_options_down.dds" mouseOver="esoui/art/chatwindow/chat_options_over.dds"/>
					<OnClicked></OnClicked>
				</Button>
				<Backdrop name="$(parent)GearPanel" centerColor="282828" edgeColor="00000000" inherits="ZO_ScrollContainerBase">
					<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="10" offsetY="35"/><Dimensions x="650" y="224"/><Edge edgeSize="1"/>
					<OnInitialized>ZO_Scroll_Initialize(self)</OnInitialized>
					<Controls>
						<Control name="$(parent)ScrollChild" resizeToFitDescendents="true">
							<OnInitialized>self:SetParent(self:GetParent():GetNamedChild("Scroll"));self:SetAnchor(3,nil,3,0,0)</OnInitialized>
						</Control>
					</Controls>
				</Backdrop>
				<Backdrop name="$(parent)SkillPanel" centerColor="282828" edgeColor="00000000" inherits="ZO_ScrollContainerBase">
					<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="346" offsetY="269"/><Dimensions x="314" y="539"/><Edge edgeSize="1"/>
					<OnInitialized>ZO_Scroll_Initialize(self)</OnInitialized>
					<Controls>
						<Control name="$(parent)ScrollChild" resizeToFitDescendents="true">
							<OnInitialized>self:SetParent(self:GetParent():GetNamedChild("Scroll"));self:SetAnchor(3,nil,3,0,0)</OnInitialized>
						</Control>
					</Controls>
				</Backdrop>
				<Backdrop name="$(parent)SetPanel" centerColor="282828" edgeColor="00000000" inherits="ZO_ScrollContainerBase">
					<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="10" offsetY="269"/><Dimensions x="327" y="539"/><Edge edgeSize="1"/>
					<OnInitialized>ZO_Scroll_Initialize(self)</OnInitialized>
					<Controls>
						<Control name="$(parent)ScrollChild" resizeToFitDescendents="true">
							<OnInitialized>self:SetParent(self:GetParent():GetNamedChild("Scroll"));self:SetAnchor(3,nil,3,0,0)</OnInitialized>
						</Control>
					</Controls>
				</Backdrop>
				<Backdrop name="$(parent)EditPanel" centerColor="282828" edgeColor="00000000" hidden="true">
					<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="10" offsetY="269"/><Dimensions x="327" y="539"/><Edge edgeSize="1"/>
					<Controls>
						<Label name="$(parent)Headline" font="AGFont" color="FFAA33"><Anchor point="1" relativePoint="1" relativeTo="$(parent)" offsetY="5"/></Label>
						<Button name="$(parent)CloseButton" clickSound="Click">
							<Anchor point="9" relativePoint="9" relativeTo="$(parent)" offsetX="-5" offsetY="4"/><Dimensions y="25" x="25"/>
							<Textures normal="esoui/art/buttons/decline_up.dds" pressed="esoui/art/buttons/decline_down.dds" mouseOver="esoui/art/buttons/decline_over.dds"/>
							<OnClicked>AG4.ShowEditPanel()</OnClicked>
						</Button>
						<Button name="$(parent)SetName" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="AGFont">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="42"/><Dimensions x="323" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Control name="$(parent)JewelryBox"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="33" offsetY="84"/><Dimensions x="124" y="40"/></Control>
						<Control name="$(parent)GearBox"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="33" offsetY="126"/><Dimensions x="292" y="40"/></Control>
						<Button name="$(parent)GearConnector" clickSound="Click" horizontalAlignment="1" verticalAlignment="1" font="AGFontBold">
							<Anchor point="9" relativePoint="9" relativeTo="$(parent)" offsetX="-2" offsetY="84"/><Dimensions x="166" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Button name="$(parent)GearLock" clickSound="Click">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="84"/><Dimensions x="29" y="82"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
							<Controls><Texture name="$(parent)Tex" textureFile="AlphaGearX2/unlocked.dds"><Anchor point="128"/><Dimensions x="32" y="32"/></Texture></Controls>
						</Button>
						
						<Button name="$(parent)Bar1Name" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="AGFont">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="176"/><Dimensions x="323" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Button name="$(parent)Bar1Icon" clickSound="Click">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="218"/><Dimensions x="71" y="82"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Control name="$(parent)W1Box"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="75" offsetY="218"/><Dimensions x="82" y="40"/></Control>
						<Control name="$(parent)Skill1Box"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="75" offsetY="260"/><Dimensions x="250" y="40"/></Control>
						<Button name="$(parent)Bar1Connector" clickSound="Click" horizontalAlignment="1" verticalAlignment="1" font="AGFontBold">
							<Anchor point="9" relativePoint="9" relativeTo="$(parent)" offsetX="-2" offsetY="218"/><Dimensions x="166" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						
						<Button name="$(parent)Bar2Name" clickSound="Click" horizontalAlignment="0" verticalAlignment="1" font="AGFont">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="310"/><Dimensions x="323" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Button name="$(parent)Bar2Icon" clickSound="Click">
							<Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="2" offsetY="352"/><Dimensions x="71" y="82"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
						<Control name="$(parent)W2Box"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="75" offsetY="352"/><Dimensions x="82" y="40"/></Control>
						<Control name="$(parent)Skill2Box"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="75" offsetY="394"/><Dimensions x="250" y="40"/></Control>
						<Button name="$(parent)Bar2Connector" clickSound="Click" horizontalAlignment="1" verticalAlignment="1" font="AGFontBold">
							<Anchor point="9" relativePoint="9" relativeTo="$(parent)" offsetX="-2" offsetY="352"/><Dimensions x="166" y="40"/>
							<Textures normal="AlphaGearX2/grey.dds" mouseOver="AlphaGearX2/light.dds"/>
							<OnClicked></OnClicked>
						</Button>
					</Controls>
				</Backdrop>
			</Controls>
		</TopLevelControl>
		
		<Button name="AG_Repair" clickSound="Click" hidden="true">
			<Anchor point="3" relativePoint="9" relativeTo="ActionButton8" offsetX="5"/><Dimensions x="50" y="50"/>
			<OnInitialized>self:SetParent(ZO_ActionBar1)</OnInitialized>
			<Controls>
				<Backdrop name="$(parent)Bg" edgeColor="00000000"><Center file="AlphaGearX2/info.dds"/><AnchorFill/><Edge edgeSize="1"/></Backdrop>
				<Label name="$(parent)Cost" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="4" relativePoint="1" relativeTo="$(parent)" offsetY="-5"/><Dimensions x="50" y="12"/>
				</Label>
				<Label name="$(parent)Value" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="1" relativePoint="4" relativeTo="$(parent)" offsetY="-1"/><Dimensions x="50" y="12"/>
				</Label>
				<Texture name="$(parent)Tex" textureFile="AlphaGearX2/gear.dds" color="00FF00"><Anchor point="128"/><Dimensions x="40" y="40"/></Texture>
			</Controls>
		</Button>
		<Button name="AG_Charge1" clickSound="Click" hidden="true">
			<Anchor point="3" relativePoint="9" relativeTo="AG_Repair" offsetX="5"/><Dimensions x="50" y="50"/>
			<OnInitialized>self:SetParent(ZO_ActionBar1)</OnInitialized>
			<Controls>
				<Backdrop name="$(parent)Bg" edgeColor="00000000"><Center file="AlphaGearX2/info.dds"/><AnchorFill/><Edge edgeSize="1"/></Backdrop>
				<Label name="$(parent)Cost" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="4" relativePoint="1" relativeTo="$(parent)" offsetY="-5"/><Dimensions x="50" y="12"/>
				</Label>
				<Label name="$(parent)Value" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="1" relativePoint="4" relativeTo="$(parent)" offsetY="-1"/><Dimensions x="50" y="12"/>
				</Label>
				<Texture name="$(parent)Tex" color="FFFFFF"><Anchor point="128"/><Dimensions x="40" y="40"/></Texture>
			</Controls>
		</Button>
		<Button name="AG_Charge2" clickSound="Click" hidden="true">
			<Anchor point="3" relativePoint="9" relativeTo="AG_Charge1" offsetX="5"/><Dimensions x="50" y="50"/>
			<OnInitialized>self:SetParent(ZO_ActionBar1)</OnInitialized>
			<Controls>
				<Backdrop name="$(parent)Bg" edgeColor="00000000"><Center file="AlphaGearX2/info.dds"/><AnchorFill/><Edge edgeSize="1"/></Backdrop>
				<Label name="$(parent)Cost" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="4" relativePoint="1" relativeTo="$(parent)" offsetY="-5"/><Dimensions x="50" y="12"/>
				</Label>
				<Label name="$(parent)Value" font="ZoFontGameSmall" color="FFFFFF" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="1" relativePoint="4" relativeTo="$(parent)" offsetY="-1"/><Dimensions x="50" y="12"/>
				</Label>
				<Texture name="$(parent)Tex" color="FFFFFF"><Anchor point="128"/><Dimensions x="40" y="40"/></Texture>
			</Controls>
		</Button>
		
		<TopLevelControl name="AG_SetTip" hidden="true"><Anchor point="3" relativePoint="9" relativeTo="AG_Panel"/><Dimensions x="400" y="400"/>
			<Controls>
				<Backdrop name="$(parent)Bg" centerColor="282828" edgeColor="000000" hidden="true">	<Edge edgeSize="1"/><AnchorFill/></Backdrop>
				<Label name="$(parent)Name" font="AGFontBold" color="FFAA33" horizontalAlignment="1" verticalAlignment="1">
					<Anchor point="1" relativePoint="1" relativeTo="$(parent)" offsetY="5"/><Dimensions x="400" y="30"/>
				</Label>
				<Backdrop name="$(parent)Skills1" centerColor="383838" edgeColor="00000000" hidden="true">
					<Anchor point="1" relativePoint="1" relativeTo="$(parent)" offsetY="37"/><Dimensions x="400" y="82"/><Edge edgeSize="1"/>
					<Controls>
						<Texture name="$(parent)Skill1Icon" color="FFFFFF"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="2"/><Dimensions x="64" y="64"/></Texture>
						<Label name="$(parent)Bar1Name" font="AGFont" color="FFAA33" verticalAlignment="1"><Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="72" offsetY="2"/><Dimensions x="265" y="40"/></Label>
						<Label name="$(parent)Bar1Skills"><Anchor point="6" relativePoint="6" relativeTo="$(parent)" offsetX="72" offsetY="-2"/><Dimensions x="265" y="40"/></Label>
					</Controls>
				</Backdrop>
				<Backdrop name="$(parent)Skills2" centerColor="383838" edgeColor="00000000" hidden="true">
					<Anchor point="1" relativePoint="1" relativeTo="$(parent)" offsetY="121"/><Dimensions x="400" y="82"/><Edge edgeSize="1"/>
					<Controls>
						<Texture name="$(parent)Skill2Icon" color="FFFFFF"><Anchor point="2" relativePoint="2" relativeTo="$(parent)" offsetX="2"/><Dimensions x="64" y="64"/></Texture>
						<Label name="$(parent)Bar2Name" font="AGFont" color="FFAA33" verticalAlignment="1"><Anchor point="3" relativePoint="3" relativeTo="$(parent)" offsetX="72" offsetY="2"/><Dimensions x="265" y="40"/></Label>
						<Label name="$(parent)Bar2Skills"><Anchor point="6" relativePoint="6" relativeTo="$(parent)" offsetX="72" offsetY="-2"/><Dimensions x="265" y="40"/></Label>
					</Controls>
				</Backdrop>
			</Controls>
		</TopLevelControl>
		
        <TopLevelControl name="AlphaGearX2">
            <OnInitialized></OnInitialized>
			<OnUpdate>AG4.Queue()</OnUpdate>
        </TopLevelControl>
    </Controls>
</GuiXml>
